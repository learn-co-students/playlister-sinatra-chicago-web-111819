<h1><%= @artist.name %></h1>
<% @artist.songs.each do |song| %>
    <p><a href="/songs/<%= song.slug %>"><%= song.name %></a></p>
    <% song.genres.each do |genre| %>
        <p><pre>    <a href="/genres/<%= genre.slug %>"><%= genre.name %></a></pre></p>
    <% end %>
<% end %>

<a href="/artists">Back to Artists List</a>